<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/11.0.14-internal" fx:controller="ru.ac.uniyar.simplex.controllers.SimplexController">
    <MenuBar>
        <VBox.margin>
            <Insets bottom="15.0"/>
        </VBox.margin>
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" text="Close"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" text="Delete"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About"/>
        </Menu>
    </MenuBar>
    <Label fx:id="welcomeText">
        <font>
            <Font size="14.0"/>
        </font>
        <padding>
            <Insets bottom="10.0"/>
        </padding>
        <VBox.margin>
            <Insets bottom="5.0"/>
        </VBox.margin>
    </Label>
    <Label fx:id="base">
        <font>
            <Font size="14.0"/>
        </font>
        <VBox.margin>
            <Insets bottom="10.0"/>
        </VBox.margin>
    </Label>
    <GridPane fx:id="table" alignment="CENTER" hgap="10.0" vgap="10.0">
        <VBox.margin>
            <Insets bottom="20.0"/>
        </VBox.margin>
    </GridPane>
    <Button mnemonicParsing="false" onAction="#nextStep" text="Продолжить"/>
</VBox>
