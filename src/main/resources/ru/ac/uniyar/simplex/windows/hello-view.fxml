<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="220.0" prefWidth="300.0" spacing="10.0"
      xmlns="http://javafx.com/javafx/11.0.14-internal"
      fx:controller="ru.ac.uniyar.simplex.controllers.HelloController">
    <padding>
        <Insets bottom="10.0" top="30.0"/>
    </padding>
    <Label fx:id="welcomeText" alignment="TOP_RIGHT" text="Добро пожаловать!">
        <font>
            <Font size="18.0"/>
        </font>
        <VBox.margin>
            <Insets bottom="20.0"/>
        </VBox.margin>
    </Label>
    <Button fx:id="enter" focusTraversable="false" mnemonicParsing="false" onAction="#onEnterButtonClick"
            prefWidth="190.0" text="Ввести задачу вручную"/>
    <Button fx:id="download" focusTraversable="false" mnemonicParsing="false" onAction="#onDownloadButtonClick"
            prefWidth="190.0" text="Загрузить задачу из файла"/>
    <Button fx:id="about" focusTraversable="false" mnemonicParsing="false" onAction="#onAboutButtonClick"
            prefWidth="190.0" text="О приложении"/>
</VBox>
