<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="350.0" prefWidth="600.0"
      xmlns="http://javafx.com/javafx/11.0.14-internal">
    <padding>
        <Insets top="20.0"/>
    </padding>
    <Label text="Симплекс метод - справка">
        <font>
            <Font size="18.0"/>
        </font>
        <VBox.margin>
            <Insets bottom="20.0"/>
        </VBox.margin>
    </Label>
    <Label prefWidth="520.0"
           text="1. Светло-голубым подсвечиваются поля, одно из которых можно выбрать как опорный элемент для следующего шага"
           wrapText="true">
        <VBox.margin>
            <Insets bottom="10.0"/>
        </VBox.margin>
